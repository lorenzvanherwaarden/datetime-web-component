    }
  },
  "dependencies": {
    "@floating-ui/dom": "^1.0.0"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "prettier": "2.8.1",
    "release-it": "^15.6.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  }
}

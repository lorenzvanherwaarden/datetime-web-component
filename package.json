{
  "name": "datetime-web-component",
  "version": "0.1.6",
  "description": "Datetime picker web component",
  "homepage": "https://github.com/lorenzvanherwaarden/datetime-web-component",
  "files": [
    "dist"
  ],
  "main": "./dist/datetime-web-component.umd.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/lorenzvanherwaarden/datetime-web-component.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format": "prettier --write .",
    "release": "release-it --only-version"
